{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    "inline-react-svg",
    "@babel/plugin-transform-runtime",
    [
      "babel-plugin-root-import",
      {
        "paths": [
          {
            "rootPathSuffix": "./src/components",
            "rootPathPrefix": "@components/"
          },
          {
            "rootPathSuffix": "./public/static/assets/images",
            "rootPathPrefix": "@images/"
          },
          {
            "rootPathSuffix": "./public/static/assets/svgs",
            "rootPathPrefix": "@svgs/"
          },
          {
            "rootPathSuffix": "./src/containers",
            "rootPathPrefix": "@containers/"
          },
          {
            "rootPathSuffix": "./src/routes/index",
            "rootPathPrefix": "@routes"
          },
          {
            "rootPathSuffix": "./src/store",
            "rootPathPrefix": "@store/"
          },
          {
            "rootPathSuffix": "./src/util/country",
            "rootPathPrefix": "@country/"
          },
          {
            "rootPathSuffix": "./src/util/hooks",
            "rootPathPrefix": "@hooks/"
          },
          {
            "rootPathSuffix": "./src/util/shared",
            "rootPathPrefix": "@shared/"
          },
          {
            "rootPathSuffix": "./src/util/axios",
            "rootPathPrefix": "@axios"
          },
          {
            "rootPathSuffix": "./src/util/lazy",
            "rootPathPrefix": "@lazy"
          }
        ]
      }
    ]
  ]
}
